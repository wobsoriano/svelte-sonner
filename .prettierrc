{
    "useTabs": true,
    "singleQuote": true,
    "trailingComma": "none",
    "tabWidth": 4,
    "printWidth": 100,
    "plugins": [
        "prettier-plugin-svelte"
    ],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte",
                "printWidth": 80
            }
        }
    ]
}